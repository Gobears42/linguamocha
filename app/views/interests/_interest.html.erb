<fieldset id="interest_<%= interest.id %>" >
	<li class="interest <%= interest.id %> skill" >
	   <p class="skill-name"><%= interest.tag %></p>
       <p class="skill-description"><%= interest.description %></p>
	</li>

	<%= link_to image_tag('trash-can-icon.jpg', size: '20x20' ), interest_path(interest.id), method: 'delete', remote: :true %>
	<%= link_to image_tag('pencil.png', size: '20x20'), edit_interest_path(interest.id), remote: :true %> 
</fieldset>